CallHandlerResult InnerFuncHandler(Location location, FunctionCall functionCall)
{
    var arg1 = FunctionCall.Arguments[0];
    var arg2 = FunctionCall.Arguments[1];
    var sum = (arg1 + arg2);
    var sub = (arg1 - arg2);
    var mul = (arg1 * arg2);
    var div = (arg1 / arg2);
    var parens = (sum - sub);
